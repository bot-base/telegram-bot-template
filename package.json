{
  "name": "telegram-bot-template",
  "version": "0.1.0",
  "description": "Telegram bot starter template",
  "main": "dist/run.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "npm run clean && tsc",
    "dev": "tsc-watch --onSuccess \"npm run start\"",
    "start": "node dist/run.js",
    "prepare": "husky install"
  },
  "author": "deptyped <deptyped@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@grammyjs/fluent": "1.0.3",
    "@grammyjs/menu": "1.1.1",
    "@grammyjs/parse-mode": "1.1.2",
    "@grammyjs/ratelimiter": "1.1.5",
    "@grammyjs/transformer-throttler": "1.1.2",
    "@grammyjs/types": "2.7.1",
    "@moebius/fluent": "1.1.0",
    "@prisma/client": "3.14.0",
    "@satont/grammy-redis-storage": "1.2.0",
    "dotenv": "16.0.1",
    "envalid": "7.3.1",
    "fastify": "3.29.0",
    "grammy": "1.8.3",
    "grammy-guard": "0.3.1",
    "ioredis": "5.0.4",
    "iso-639-1": "2.1.13",
    "lodash": "4.17.21",
    "module-alias": "2.2.2",
    "node-fetch": "3.2.4",
    "pino": "7.11.0",
    "pino-pretty": "7.6.1",
    "prom-client": "14.0.1"
  },
  "devDependencies": {
    "@types/debug": "4.1.7",
    "@types/ioredis": "4.28.10",
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.30",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "eslint": "8.15.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "8.0.1",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "prisma": "3.14.0",
    "rimraf": "3.0.2",
    "ts-node": "10.7.0",
    "tsc-watch": "5.0.3",
    "typescript": "4.6.4"
  },
  "lint-staged": {
    "*.ts": "npm run lint"
  },
  "_moduleAliases": {
    "@bot": "dist"
  }
}
