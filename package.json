{
  "name": "telegram-bot-template",
  "version": "0.1.0",
  "description": "Telegram bot starter template",
  "type": "module",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "format": "eslint \"src/**/*.ts\" --fix",
    "typecheck": "tsc",
    "dev": "tsc-watch --onSuccess \"tsx ./scripts/start.ts\"",
    "start": "tsc && tsx ./scripts/start.ts",
    "start:force": "tsx ./scripts/start.ts",
    "prepare": "npx husky install"
  },
  "author": "deptyped <deptyped@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@grammyjs/auto-chat-action": "0.1.1",
    "@grammyjs/hydrate": "1.3.1",
    "@grammyjs/i18n": "1.0.1",
    "@grammyjs/parse-mode": "1.7.1",
    "@grammyjs/storage-redis": "2.3.0",
    "@grammyjs/types": "3.1.2",
    "@prisma/client": "5.0.0",
    "callback-data": "1.0.2",
    "dotenv": "16.3.1",
    "fastify": "4.21.0",
    "grammy": "1.17.2",
    "grammy-guard": "0.5.0",
    "ioredis": "5.3.2",
    "iso-639-1": "2.1.15",
    "lodash": "4.17.21",
    "node-graceful-shutdown": "1.1.5",
    "pino": "8.14.2",
    "pino-pretty": "10.2.0",
    "prom-client": "14.2.0",
    "tsx": "3.12.7",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/debug": "4.1.8",
    "@types/ioredis": "4.28.10",
    "@types/lodash": "4.14.196",
    "@types/node": "20.4.5",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "eslint": "8.46.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-unicorn": "47.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "prettier": "3.0.0",
    "prettier-plugin-organize-imports": "3.2.3",
    "prisma": "5.0.0",
    "rimraf": "5.0.1",
    "tsc-watch": "6.0.4",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.ts": "npm run lint"
  }
}