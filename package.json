{
  "name": "telegram-bot-template",
  "version": "0.1.0",
  "description": "Telegram bot starter template",
  "main": "dist/run.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "watch": "tsc --watch --preserveWatchOutput --pretty",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "npm run clean && tsc",
    "dev": "npm run build && concurrently -i -k -n \"bot,build\" -c \"blue.bold,yellow.bold\" \"nodemon --watch dist\" \"npm run watch\"",
    "start": "node dist/run.js"
  },
  "author": "deptyped <deptyped@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@grammyjs/menu": "1.0.4",
    "@grammyjs/ratelimiter": "1.1.4",
    "@grammyjs/transformer-throttler": "1.0.1",
    "@moebius/fluent": "0.2.0",
    "@moebius/grammy-fluent": "0.3.0",
    "@prisma/client": "3.7.0",
    "@satont/grammy-redis-storage": "1.1.1",
    "envalid": "7.2.2",
    "fastify": "3.25.3",
    "grammy": "1.5.4",
    "ioredis": "4.28.2",
    "iso-639-1": "2.1.11",
    "lodash": "4.17.21",
    "module-alias": "2.2.2",
    "node-fetch": "3.1.0",
    "pino": "7.6.1",
    "prom-client": "14.0.1"
  },
  "devDependencies": {
    "@types/debug": "4.1.7",
    "@types/ioredis": "4.28.5",
    "@types/lodash": "4.14.178",
    "@types/node": "17.0.4",
    "concurrently": "6.5.1",
    "nodemon": "2.0.15",
    "pino-pretty": "7.3.0",
    "prettier": "2.5.1",
    "prisma": "3.7.0",
    "rimraf": "3.0.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "_moduleAliases": {
    "@bot": "dist"
  }
}
